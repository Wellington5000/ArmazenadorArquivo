<div class="Form">
    <div id="Form" hidden>
        <form action="/home" method="POST" name="formulario">
            <div class="form-group">
                <div class="   col-md-6 offset-md-3">
                    <label> Número</label>
                    <input type="text" name="login" class="form-control " placeholder=" Número">
                </div>
            </div>

            <div class="form-group">
                <div class="col-md-6 offset-md-3">
                    <label> CPF/CNPJ </label>
                    <input type="text" name="rg" class="form-control" placeholder="Número do CPF/CNPJ">
                </div>
            </div>

            <div class="form-group">
                <div class="   col-md-6 offset-md-3">
                    <label> Data de Emissão</label>
